// Online C++ compiler to run C++ program online
#include <iostream>
#include<string>
#include<vector>
#include<unordered_map>
using namespace std;
string removechar(string s,char c)
{
    int n = s.length();
    string ans;
    for(int i=0;i<n;i++)
    {
        if(s[i]!=c)
        {
            ans += s[i];
        }
    }
    return ans;
}
int main() {
    // Write C++ code here
    string s = "hiiiiaaap";
    cout<<removechar(s,'i');

    
}
