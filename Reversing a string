// Online C++ compiler to run C++ program online
#include <iostream>
#include<string>
using namespace std;
void rev(string &s)
{
    int start =0;
    int end = s.length()-1;
    while(start<end)
    {
        swap(s[start],s[end]);
        start++;
        end--;
    }
    return;
}
int main() {
    // Write C++ code here
    string s = "hello";
    rev(s);
    cout<<s;
    

    return 0;
}
